extends ../src/template/pageDefault.pug
block vars
    - var config = {title:"接送机", body:{cls:""},keywords:'接送机',description:'接送机服务'}
block css
    link(href='css/travel.css',rel='stylesheet')
    link(href='css/component-antd.css',rel='stylesheet')
//- 页面主体内容
block mainContent
    .box(style='margin:0 auto;padding:20px 100px')
        fieldset(class='layui-elem-field layui-field-title',style='margin-top: 20px;')
            legend 旅游度假改版常用组件(还在完善中...)
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 1、layer简单弹框提示
                .layui-colla-content.layui-show
                    button#msgBtn(class="layui-btn layui-btn-normal") 简单弹框
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 2、layer有确定取消按钮的弹框提示
                .layui-colla-content.layui-show
                    button#msgOpen(class="layui-btn layui-btn-normal") 确定取消按钮的弹框
        .layui-collapse(style='display:none')
            .layui-colla-item
                .layui-colla-title 3、常用按钮样式
                .layui-colla-content.layui-show
                    button(class="layui-btn layui-btn-primary") 原始按钮
                    button(class="layui-btn") 默认按钮
                    button(class="layui-btn layui-btn-normal") 百搭按钮
                    button(class="layui-btn layui-btn-warm") 暖色按钮
                    button(class="layui-btn layui-btn-danger") 警告按钮
                    button(class="layui-btn layui-btn-disabled") 禁用按钮
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 4、区域loading
                .layui-colla-content.layui-show
                    #loadingBox(style='height:400px;position:relative;')
                    button#msgloading(class="layui-btn") loading(点击)
                    button#msgLoadingClose(class="layui-btn") loading(关闭)
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 5、全屏loading
                .layui-colla-content.layui-show
                   button#fullloading(class="layui-btn") loading(点击)
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 6、无数据显示
                .layui-colla-content.layui-show
                    #noDataBox
                    button#showNoData(class="layui-btn") 显示无数据显示
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 7、日历控件
                .layui-colla-content.layui-show
                    input(class='inputCss',id='inputDate',value='2016-10-18',readOnly)
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 8、控件验证
                .layui-colla-content.layui-show
                    .msgBox
                        input(class='inputCss',type='text',rule='{"required":true}',style='border:1px solid gray')
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 9、模拟下拉菜单
                .layui-colla-content.layui-show
                    .selectModel
                        span.select 2 成人
                        ul.downList(style='display: none;')
                            li(value='1') 1 成人
                            li(value='1') 2 成人
                            li(value='1') 3 成人
                            li(value='1') 4 成人
                        input(type='hidden',value='2',name='adtCount')
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 10、模拟checkBox框
                .layui-colla-content.layui-show
                        span.checkBox 我已经阅读并同意
                        br
                        span.checkBox.active 我已经阅读并同意
        fieldset(class='layui-elem-field layui-field-title',style='margin-top: 20px;')
            legend 旅游度假改版通用样式(还在完善中...)
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 1、改版页面按钮集合
                .layui-colla-content.layui-show
                    a.searchBtn 搜索
                    | &nbsp;
                    a.smallSearchBtn 搜索
                    | &nbsp;
                    a.blueSmallBtn 确定
                    | &nbsp;
                    a.graySmallBtn 取消
                    | &nbsp;
                    a.yellowSmallBtn 搜索
                    | &nbsp;
                    a.yellowBtn
                      | 立即预定
                      i &gt;
                    | &nbsp;
                    a.yellowBtn 确定
                    | &nbsp;
                    a.backBtn 返回修改
                    | &nbsp;
                    a.nextBtn
                       | 下一步
                       i.arrow
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 2、阶段步骤样式
                .layui-colla-content.layui-show
                      .newStep
                          .step.step01.done
                              i.ico
                              span.text 选择资源
                          .step.step02.doing
                              i.ico
                              span.text 填写订单
                          .step.step03
                              i.ico
                              span.text 提交订单
        .layui-collapse
            .layui-colla-item
                .layui-colla-title 3、输入框
                .layui-colla-content.layui-show
                    input.inputCss(type='text',placeholder='城市名')
                    | &nbsp;
                    input.inputCss.date(type='text',placeholder='输入日期')

//-页面使用到的js
block js
    script(src='js/component.min.js',type='text/javascript')